{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./app/*"],
      "@android/*": ["./app/(android)/*"],
      "@web/*": ["./app/(web)/*"],
      "@components/*": ["./components/*"],
      "@assets/*": ["./assets/*"],
      "@hooksC/*": ["./hooks/config/*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts"
  ]
}